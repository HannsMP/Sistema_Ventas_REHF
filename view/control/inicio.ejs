<div class="content-header">
  <!---------------------- CSS ---------------------->

  <!-- lib control -->
  <link href="/src/private/lib/dataTables/datatables.min.css" rel="stylesheet">
  <link href="/src/private/lib/selector/selector.min.css" rel="stylesheet">
  <link href="/src/private/lib/imagen/imagen.min.css" rel="stylesheet">
  <link href="/src/private/lib/catalogue/catalogue.css" rel="stylesheet">

  <!-- layout -->
  <link href="/src/private/layout/css/form.css" rel="stylesheet">
  <link href="/src/private/layout/css/input.css" rel="stylesheet">
  <link href="/src/private/layout/css/btn.css" rel="stylesheet">

  <!---------------------- JS ---------------------->

  <!-- lib control -->
  <script src="/src/private/lib/chart/chart.umd.min.js"></script>
  <script src="/src/private/lib/selector/selector.min.js"></script>
  <script src="/src/private/lib/imagen/imagen.min.js"></script>

  <!-- layout -->
  <script src="/src/private/lib/input/input.min.js"></script>

  <!---------------------- BODY ---------------------->

  <div class="content-tittle">Titulo</div>
  <div class="content-path">
    <a href="/control">/control</a>
    <a href="/control/inicio">/inicio</a>
  </div>
</div>

<div class="content-body">
  <div class="menu-body">
    <div class="card">
      <div class="card-head">
        <h3 class="card-tittle">Categorias</h3>
        <a class="btn btn-info"><i class='bx bx-plus'></i></a>
      </div>
      <div class="card-body">
        <div class="table-box">
          <table id="table-main" style="width: auto;">
            <thead>
              <tr>
                <th>Ajustes</th>
                <th>Nombre</th>
                <th>Edad</th>
                <th>Ciudad</th>
                <th>Ocupacion</th>
                <th>Fecha</th>
                <th>Salario</th>
              </tr>
            </thead>
            <tbody> </tbody>
            <tfoot>
              <tr>
                <th>Ajustes</th>
                <th>Nombre</th>
                <th>Edad</th>
                <th>Ciudad</th>
                <th>Ocupacion</th>
                <th>Fecha</th>
                <th>Salario</th>
              </tr>
            </tfoot>
          </table>

          <div class="tables-utils">
            <div class="download"></div>
            <div class="option">
              <input class="check-switch" type="checkbox">
              <a class="btn btn-danger"><i class='bx bx-trash'></i></a>
            </div>
          </div>
        </div>
      </div>
    </div>
    <div class="cards">
      <div class="card">
        <div class="card-head">
          <h3 class="card-tittle">worldwide-sales</h3>
        </div>
        <div class="card-body">
          <canvas id="worldwide-sales"></canvas>
        </div>
      </div>

      <div class="card">
        <div class="card-head">
          <h3 class="card-tittle">salse-revenue</h3>
        </div>
        <div class="card-body">
          <canvas id="salse-revenue"></canvas>
        </div>
      </div>

      <div class="card">
        <div class="card-head">
          <h3 class="card-tittle">line-chart</h3>
        </div>
        <div class="card-body">
          <canvas id="line-chart"></canvas>
        </div>
      </div>

      <div class="card">
        <div class="card-head">
          <h3 class="card-tittle">bar-chart</h3>
        </div>
        <div class="card-body">
          <canvas id="bar-chart"></canvas>
        </div>

      </div>

      <div class="card">
        <div class="card-head">
          <h3 class="card-tittle">pie-chart</h3>
        </div>
        <div class="card-body">
          <canvas id="pie-chart"></canvas>
        </div>
      </div>

      <div class="card">
        <div class="card-head">
          <h3 class="card-tittle">doughnut-chart</h3>
        </div>
        <div class="card-body">
          <canvas id="doughnut-chart"></canvas>
        </div>
      </div>
    </div>
  </div>
  <div class="menu-side">
    <div class="card">
      <span class="upd-sz"></span>
    </div>
    <div class="card">
      <div class="card-head">
        <h3 class="card-tittle">Nuevo Producto</h3>
        <a class="card-close"><i class='bx bx-x'></i></a>
      </div>
      <div class="card-body">
        <div class="card-form">
          <div class="form-box">
            <form>
              <div class="form-body">
                <div class="form-col">
                  <input type="hidden" name="id">
                  <div class="input-box">
                    <input name="nombre" type="text" required>
                    <label for="nombre">Nombre:</label>
                  </div>
                  <div class="input-box">
                    <input name="limite" type="text" oninput="inputCapitalize(this, 10)" required>
                    <label for="limite">Limite:</label>
                  </div>
                  <div class="input-box">
                    <input name="capitalizar" type="text" oninput="inputCapitalize(this, 100)" required>
                    <label for="capitalizar">Capitalizar:</label>
                  </div>
                  <div class="input-box">
                    <input name="mayusculas" type="text" oninput="inputUppercase(this, 100)" required>
                    <label for="mayusculas">Mayusculas:</label>
                  </div>
                  <div class="input-box">
                    <input name="minusculas" type="text" oninput="inputLowercase(this, 100)" required>
                    <label for="minusculas">Minusculas:</label>
                  </div>
                  <div class="input-box">
                    <input name="oraciones" type="text" oninput="inputSentenceCase(this, 100)" required>
                    <label for="oraciones">Oraciones:</label>
                  </div>
                  <div class="input-box">
                    <input name="parrafos" type="text" oninput="inputParagraphCase(this, 100)" required>
                    <label for="parrafos">Parrafos:</label>
                  </div>
                  <div class="input-box">
                    <input name="edad" type="text" oninput="inputInt(this)" required>
                    <label for="edad">Edad:</label>
                  </div>
                  <div class="input-box">
                    <input name="email" type="email" required>
                    <label for="email">Email:</label>
                  </div>
                </div>
                <div class="form-col">
                  <div class="input-box">
                    <input name="contraseña" type="password" required>
                    <label for="contraseña">Contraseña:</label>
                  </div>
                  <div class="input-box">
                    <textarea name="descripcion" class="scroll-y" oninput="autoHeight(this)" required></textarea>
                    <label for="descripcion">Descripcion:</label>
                  </div>
                  <div class="input-box">
                    <textarea name="descripcionLimit" class="scroll-y" oninput="autoHeight(this); inputLimit(this, 10);"
                      required></textarea>
                    <label for="descripcionLimit">Descripcion(10):</label>
                  </div>
                  <div class="input-box">
                    <input name="precion" type="text" oninput="inputFloat(this)" required>
                    <label for="precion">Precio:</label>
                  </div>
                  <div class="input-box">
                    <input class="selector" type="search" placeholder="Buscar categoria...">
                    <label for="categorias">Categorias:</label>
                  </div>
                  <div class="input-box">
                    <input class="imagen-unic" type="file" accept=".jpg, .png">
                    <label for="fotos">Nombre:</label>
                  </div>
                  <div class="input-box">
                    <div class="checked-box">
                      <input class="check-radio" name="nivel" type="radio" checked>
                      <span>Alto</span>
                    </div>
                    <div class="checked-box">
                      <input class="check-radio" name="nivel" type="radio">
                      <span>Normal</span>
                    </div>
                    <div class="checked-box">
                      <input class="check-radio" name="nivel" type="radio">
                      <span>Bajo</span>
                    </div>
                    <label for="nivel">Nivel:</label>
                  </div>
                  <div class="input-box">
                    <div class="checked-box">
                      <input class="check-switch" type="checkbox">
                      <span>off</span>
                    </div>
                    <div class="checked-box">
                      <input class="check-switch" type="checkbox" checked>
                      <span>on</span>
                    </div>
                    <div class="checked-box">
                      <input class="check-switch" type="checkbox" disabled>
                      <span>off disabled</span>
                    </div>
                    <div class="checked-box">
                      <input class="check-switch" type="checkbox" checked disabled>
                      <span>on disabled</span>
                    </div>
                    <label for="nivel">checked-switch:</label>
                  </div>
                  <div class="input-box">
                    <div class="checked-box">
                      <input class="check-checked" type="checkbox">
                      <span>off</span>
                    </div>
                    <div class="checked-box">
                      <input class="check-checked" type="checkbox" checked>
                      <span>on</span>
                    </div>
                    <div class="checked-box">
                      <input class="check-checked" type="checkbox" disabled>
                      <span>off disabled</span>
                    </div>
                    <div class="checked-box">
                      <input class="check-checked" type="checkbox" checked disabled>
                      <span>on disabled</span>
                    </div>
                    <label for="nivel">checked-checked:</label>
                  </div>
                </div>
              </div>
            </form>
            <div class="form-foot">
              <input class="check-switch" type="checkbox">
              <input class="check-checked" type="checkbox">
              <a class="btn btn-danger">cancelar</a>
              <a class="btn btn-warning">Editar</a>
              <a class="btn btn-success">guardar</a>
            </div>
          </div>
        </div>
      </div>
    </div>
  </div>
</div>

<script src="/src/private/lib/catalogue/Catalogue.js"></script>
<script src="/src/private/routes/test.js"></script>